{
  "name": "codacy/codacy-metrics-pdepend",
  "description": "Metrics tool for PHP based on PDepend",
  "require": {
    "pdepend/pdepend": "2.5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "\\Codacy\\PDepend\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "\\Codacy\\PDepend\\": "tests/"
    }
  }
}
